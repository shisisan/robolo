{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
			"syntax": "typescript",
			"decorators": true
		},
        "target": "es2020",
        "baseUrl": "./src",
        "paths": {
			"#/root/*": [
				"./src/*"
			],
			"#/commands/*": [
				"./commands/*"
			],
			"#/listeners/*": [
				"./listeners/*"
			],
			"#/structures/*": [
				"./structures/*"
			],
			"#/lib/*": [
				"./lib/*"
			]
		}
    },
    "module": {
        "type": "commonjs"
    }
}